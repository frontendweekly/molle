{
  "name": "@frontendweekly/molle",
  "version": "0.0.34",
  "description": "MOLLE is a plugin for Eleventy. Not for everyone to use it.",
  "keywords": [
    "eleventy",
    "eleventy-plugin"
  ],
  "author": "Yuya Saito <hello@studiomohawk.com> (https://github.com/studiomohawk)",
  "homepage": "https://github.com/frontendweekly/molle",
  "license": "MIT",
  "main": ".eleventy.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "directories": {
    "test": "example"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/frontendweekly",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendweekly/molle.git",
    "directory": "packages/molle"
  },
  "scripts": {
    "example": "ELEVENTY_ENV=test eleventy --input=example --output=example/_site --config=example/eleventy-config.js"
  },
  "bugs": {
    "url": "https://github.com/frontendweekly/molle/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "ucontent": "^2.0.0"
  },
  "gitHead": "5758ace4f9bbc84ecfe87966d53f60a1625be43a",
  "dependencies": {
    "@frontendweekly/eleventy-plugin-markdown": "workspace:packages/eleventy-plugin-markdown",
    "@frontendweekly/filter-date-iso": "workspace:packages/filter-date-iso",
    "@frontendweekly/filter-date-ordinal-suffix": "workspace:packages/filter-date-with-ordinal-suffix",
    "@frontendweekly/filter-head": "workspace:packages/filter-head",
    "@frontendweekly/filter-markdown": "workspace:packages/filter-markdown",
    "@frontendweekly/filter-tags-to-sentence": "workspace:packages/filter-tags-to-sentence",
    "@frontendweekly/transform-enhance-post-html": "workspace:packages/transform-enhance-post-html",
    "@frontendweekly/transform-htmlmin": "workspace:packages/transform-htmlmin"
  }
}
