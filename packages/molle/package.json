{
  "name": "@frontendweekly/molle",
  "version": "0.0.41",
  "description": "MOLLE is a plugin for Eleventy. Not for everyone to use it.",
  "keywords": [
    "eleventy",
    "eleventy-plugin"
  ],
  "author": "Yuya Saito <hello@studiomohawk.com> (https://github.com/studiomohawk)",
  "homepage": "https://github.com/frontendweekly/molle",
  "license": "MIT",
  "main": ".eleventy.js",
  "engines": {
    "node": ">=14.15.0"
  },
  "directories": {
    "test": "example"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/frontendweekly",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendweekly/molle.git",
    "directory": "packages/molle"
  },
  "scripts": {
    "example": "ELEVENTY_ENV=test eleventy --input=example --output=example/_site --config=example/eleventy-config.js"
  },
  "bugs": {
    "url": "https://github.com/frontendweekly/molle/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "ucontent": "^2.0.0"
  },
  "gitHead": "5758ace4f9bbc84ecfe87966d53f60a1625be43a",
  "dependencies": {
    "@frontendweekly/eleventy-plugin-markdown": "^0.0.13",
    "@frontendweekly/filter-date-iso": "^0.0.14",
    "@frontendweekly/filter-date-ordinal-suffix": "^0.0.17",
    "@frontendweekly/filter-head": "^0.0.8",
    "@frontendweekly/filter-markdown": "^0.0.19",
    "@frontendweekly/filter-tags-to-sentence": "^0.0.10",
    "@frontendweekly/transform-enhance-post-html": "^0.0.30",
    "@frontendweekly/transform-htmlmin": "^0.0.15"
  }
}
