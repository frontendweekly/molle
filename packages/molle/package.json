    "@frontendweekly/filter-head": "^0.0.4",
    "@frontendweekly/filter-markdown": "^0.0.5",
    "@frontendweekly/filter-tags-to-sentence": "^0.0.5",
    "@frontendweekly/transform-enhance-post-html": "^0.0.11",
    "@frontendweekly/transform-htmlmin": "^0.0.11",
    "markdown-it": "^11.0.0",
    "markdown-it-attribution": "^0.1.1",
    "markdown-it-classy": "^0.2.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.2"
  }
